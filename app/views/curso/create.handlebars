<div class="row mt-5">
    <h1 class="col-11">Cadastro de curso</h1>
    <button type="button" class="btn btn-primary mt-2 " aria-pressed="false"><i class="far fa-arrow-left"></i>Voltar</button>
</div>

<div class="bs-docs-section row col-8">
    <form>
    <fieldset>
        </div>
        {{!-- sigla,nome,descricao,area --}}
        <div class="form-group">
        <label for="sigla">Sigla</label>
        <input type="text" maxlength="10" class="form-control" id="sigla" placeholder="Informe a Sigla do Curso">
        </div>

        <div class="form-group">
        <label for="nome">Nome</label>
        <input type="text" maxlength="10" class="form-control" id="nome" placeholder="Informe o nome do Curso">
        </div>

        <div class="form-group">
        <label for="descricao">Descricao</label>
        <textarea class="form-control" id="descricao" rows="3"></textarea>
        </div>

        <div class="form-group">
            <div class="custom-control custom-radio">
            <input type="radio" id="customRadio1" name="customRadio" class="custom-control-input" checked="">
            <label class="custom-control-label" for="customRadio1">Humanas</label>
            </div>
            <div class="custom-control custom-radio">
            <input type="radio" id="customRadio2" name="customRadio" class="custom-control-input">
            <label class="custom-control-label" for="customRadio2">Exatas</label>
            </div>
            <div class="custom-control custom-radio">
            <input type="radio" id="customRadio3" name="customRadio" class="custom-control-input" disabled="">
            <label class="custom-control-label" for="customRadio3">Biol√≥gicas</label>
            </div>
        </div>
        <button type="submit" class="btn btn-primary mt-3">Enviar</button>
    </fieldset>
    </form>
</div>